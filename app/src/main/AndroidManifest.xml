<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.m_oopfinalproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon144"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon144"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JackpotGBKActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="jackpotgbk" />
            </intent-filter>
        </activity>
        <activity android:name=".Tube_calculatorActivitv2" />
        <activity android:name=".RandomActivity" />
        <activity android:name=".PhytagorasActivity" />
        <activity android:name=".QuadraticActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>