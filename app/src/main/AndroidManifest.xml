<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.m_oopfinalproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon144"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon144"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".JackpotGBKHelp"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".TubeCalculatorActivity" />
        <activity android:name=".RandomActivity" />
        <activity android:name=".PhytagorasActivity" />
        <activity android:name=".QuadraticActivity" />
        <activity
            android:name=".JackpotGBKActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="orientation|screenSize" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="jackpotgbk" />
            </intent-filter>
        </activity>
    </application>

</manifest>